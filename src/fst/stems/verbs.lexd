[:ch{I}\<] :Set_B OK_Verbs [<V><IV><Imp>:] Set_B: [:>oq]
[:m{I}\<] :Set_B OK_Verbs [<V><IV><NegImp>:] Set_B: [:>ok]
OK_Verbs [<V><IV><Inf>:] [:>ok]
OK_Verbs [<V><IV><Root>:]

#---------- NK Verbs ----------------
[:n{A}{K}\<] :Set_B NK_Verbs [<V><IV><Prs>:] Set_B: [:>nk]
[:x\<] :Set_B NK_Verbs [<V><IV><Pst>:] Set_B: [:>nk]
[:t{FutA}\<] :Set_B NK_Verbs [<V><IV><Fut>:] Set_B: [:>nq]
[:k{I}\<] :Set_B NK_Verbs [<V><IV><NarPst>:] Set_B: [:>nk]
