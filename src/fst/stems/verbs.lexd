LEXICON AK_Verbs
wa' # eat/comer

LEXICON OK_Verbs
b'oq # call/llamar
il # see/ver

LEXICON NK_Verbs
ab'i # to hear/oir/escuchar

LEXICON Stative_Verbs
wan
yoo

PATTERN Intransitive_Verbs
#---------- AK Verbs ----------------
[:n{A}{K}\<] :Set_B AK_Verbs [<V><IV><Prs>:] Set_B: [:>ak]
[:x\<] :Set_B AK_Verbs [<V><IV><Pst>:] Set_B: [:>ak]
[:t{FutA}\<] :Set_B AK_Verbs [<V><IV><Fut>:] Set_B: [:>aq]
[:k{I}\<] :Set_B AK_Verbs [<V><IV><NarPst>:] Set_B: [:>ak]
[:ch{I}\<] :Set_B AK_Verbs [<V><IV><Imp>:] Set_B: [:>aq]
[:m{I}\<] :Set_B AK_Verbs [<V><IV><NegImp>:] Set_B: [:>ak]
AK_Verbs [<V><IV><Inf>:] [:>ak]
AK_Verbs [<V><IV><Root>:]

#---------- OK Verbs ----------------
[:n{A}{K}\<] :Set_B OK_Verbs [<V><IV><Prs>:] Set_B: [:>ok]
[:x\<] :Set_B OK_Verbs [<V><IV><Pst>:] Set_B: [:>ok]
[:t{FutA}\<] :Set_B OK_Verbs [<V><IV><Fut>:] Set_B: [:>oq]
